{
  "name": "embed",
  "main": "src/index.tsx",
  "version": "0.0.1-canary",
  "homepage": "/channels/",
  "proxy": {
    "/api": {
      "target": "http://127.0.0.1:8443/",
      "changeOrigin": true,
      "secure": false,
      "ws": true
    }
  },
  "scripts": {
    "start": "react-app-rewired start --scripts-version react-scripts-ts",
    "build": "react-app-rewired build --scripts-version react-scripts-ts",
    "analyze": "yarn build --analyze",
    "lingui": "lingui",
    "test": "react-app-rewired test --env=jsdom --scripts-version react-scripts-ts",
    "eject": "react-app-rewired eject --scripts-version react-scripts-ts",
    "schema": "apollo schema:download --endpoint=http://127.0.0.1:8443/api/graphql src/lib/apollo/codegen/schema.json",
    "codegen:tasks": "ts-node --skip-project src/lib/apollo/codegen/tasks",
    "codegen": "apollo codegen:generate --target=typescript --addTypename --outputFlat src/__generated__",
    "codegen:live": "apollo codegen:generate --target=typescript --addTypename --outputFlat src/__generated__ --schema=\"http://127.0.0.1:8443/api/graphql\"",
    "typecheck": "tsc src/ --noEmit",
    "doc": "typedoc --includeDeclarations --excludeExternals --externalPattern \"**/node_modules/!(@widgetbot)/**\" --ignoreCompilerErrors --out ./docs/ ./src"
  },
  "dependencies": {
    "@lingui/react": "^2.8.3",
    "@widgetbot/embed-api": "^1.1.3",
    "all-contributors-cli": "^5.4.0",
    "apollo-cache-inmemory": "^1.4.2",
    "apollo-cache-persist": "^0.1.1",
    "apollo-client": "^2.4.12",
    "apollo-link-batch-http": "^1.2.8",
    "apollo-link-error": "^1.1.7",
    "apollo-link-logger": "^1.2.3",
    "apollo-link-persisted-queries": "^0.2.2",
    "apollo-link-retry": "^2.2.10",
    "apollo-link-ws": "^1.0.14",
    "autobind-decorator": "^2.4.0",
    "axios": "^0.19.0",
    "babel-core": "^6.26.3",
    "babel-plugin-emotion": "^9.2.8",
    "babel-plugin-graphql-tag": "^1.6.0",
    "babel-plugin-macros": "^2.6.1",
    "babel-plugin-preval": "^3.0.1",
    "babel-plugin-ramda": "^1.6.3",
    "babel-plugin-syntax-dynamic-import": "^6.18.0",
    "babel-plugin-transform-builtin-extend": "^1.1.2",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-react": "^6.24.1",
    "babel-preset-react-optimize": "^1.0.1",
    "babel-preset-stage-0": "^6.24.1",
    "color": "^3.1.2",
    "emotion": "^9.2.8",
    "emotion-theming": "^9.2.6",
