{
  "name": "@widgetbot/embed",
  "main": "src/index.tsx",
  "version": "0.1.0",
  "private": true,
  "homepage": "/channels/",
  "proxy": {
    "/api": {
      "target": "http://localhost:7000",
      "changeOrigin": true,
      "secure": false,
      "ws": true
    }
  },
  "scripts": {
    "start": "react-app-rewired start --scripts-version react-scripts-ts",
    "build": "react-app-rewired build --scripts-version react-scripts-ts",
    "analyze": "yarn build --analyze",
    "lingui": "lingui",
    "test": "react-app-rewired test --env=jsdom --scripts-version react-scripts-ts",
    "eject": "react-app-rewired eject --scripts-version react-scripts-ts",
    "schema": "apollo schema:download --endpoint=http://localhost:7000/api/graphql src/lib/apollo/codegen/schema.json; node src/lib/apollo/codegen/update.js",
    "codegen": "apollo codegen:generate --target=typescript --schema=\"src/lib/apollo/codegen/schema.json\" --queries=\"src/**/*.{ts,tsx}\" --clientSchema=\"src/state/schema.ts\" --addTypename",
    "typecheck": "tsc src/ --noEmit",
    "codegen:live": "yarn codegen --schema=\"http://localhost:7000/api/graphql\"",
    "doc": "typedoc --includeDeclarations --excludeExternals --externalPattern \"**/node_modules/!(@widgetbot)/**\" --ignoreCompilerErrors --out ./docs/ ./src"
  },
  "dependencies": {
    "@lingui/react": "^2.3.0",
    "@types/color": "^3.0.0",
    "@types/highlight.js": "^9.12.3",
    "@types/lingui__core": "^2.1.0",
    "@types/lingui__react": "^2.1.0",
    "@types/memoizee": "^0.4.2",
    "@types/qs": "^6.5.1",
    "@types/ramda": "^0.25.36",
    "@types/raven-js": "^3.10.0",
    "@types/rc-tooltip": "^3.4.12",
    "@types/react-custom-scrollbars": "^4.0.5",
    "@types/react-notification-system": "^0.2.37",
    "@types/react-router-dom": "^4.3.0",
    "@types/react-virtualized": "^9.18.5",
    "@types/recompose": "^0.26.3",
    "@types/socket.io-client": "^1.4.32",
    "@widgetbot/embed-api": "^1.1.3",
    "color": "^3.0.0",
    "autobind-decorator": "^2.1.0",
    "apollo-cache-inmemory": "^1.3.0-beta.6",
    "apollo-cache-persist": "^0.1.1",
    "apollo-client": "^2.4.0-alpha.13",
    "apollo-link": "^1.2.2",
    "apollo-link-batch-http": "^1.2.2",
    "apollo-link-error": "^1.1.0",
    "apollo-link-logger": "^1.2.3",
    "apollo-link-persisted-queries": "^0.2.1",
    "apollo-link-retry": "^2.2.4",
    "apollo-link-state": "^0.4.1",
    "apollo-link-ws": "^1.0.8",
    "babel-polyfill": "^6.26.0",
    "emotion": "^9.2.6",
    "emotion-theming": "^9.2.6",
    "graphql": "^0.13.2",
    "graphql-tag": "^2.9.2",
    "highlight.js": "^9.12.0",
    "immer": "^1.5.0",
    "match-sorter": "^2.2.3",
    "memoizee": "^0.4.12",
    "moment": "^2.22.2",
    "qs": "^6.5.2",
    "ramda": "^0.25.0",
    "rc-tooltip": "^3.7.2",
    "react-apollo": "^2.1.9",
    "react-app-rewired": "^1.5.2",
    "react-custom-scrollbars": "^4.2.1",
    "react-easy-emoji": "^1.2.0",
    "react-emotion": "^9.2.6",
    "react-hot-keys": "^1.2.0",
    "react-icons": "^3.0.4",
    "react-notification-system": "^0.2.17",
    "react-router-dom": "^4.3.1",
    "react-transition-group": "^2.4.0",
    "react-virtualized": "^9.20.1",
    "recompose": "0.27.1",
    "scroll-into-view-if-needed": "^2.2.16",
    "simple-markdown": "^0.4.1",
    "socket.io-client": "^2.1.1",
    "subscriptions-transport-ws": "^0.9.14",
    "typedi": "^0.8.0"
  },
  "devDependencies": {
    "@lingui/babel-preset-react": "^2.3.0",
    "@lingui/cli": "^2.3.0",
    "@types/node": "10.1.2",
    "raw-loader": "^0.5.1",
    "react-scripts-ts": "2.17.0",
    "ts-graphql-plugin": "^1.1.2",
    "typedoc": "^0.11.1",
    "webpack-bundle-analyzer": "^2.13.1"
  }
}
